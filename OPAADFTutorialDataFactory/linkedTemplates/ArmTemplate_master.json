{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "OPAADFTutorialDataFactory"
		},
		"AzureLinkedServiceG2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureLinkedServiceG2'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"AzureLinkedServiceG2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dataresource11.dfs.core.windows.net"
		},
		"Hdfs1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "D:\\NUMERICA\\Jobs\\OPIANALYTICS"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "OPAADFTutorialDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureLinkedServiceG2_accountKey": {
						"value": "[parameters('AzureLinkedServiceG2_accountKey')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzureLinkedServiceG2_properties_typeProperties_url": {
						"value": "[parameters('AzureLinkedServiceG2_properties_typeProperties_url')]"
					},
					"Hdfs1_properties_typeProperties_url": {
						"value": "[parameters('Hdfs1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}